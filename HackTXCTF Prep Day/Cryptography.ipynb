      "b'OK Boomer!'\n"
     ]
    }
   ],
   "source": [
    "# Manually get hex representation\n",
    "print(s_bytes)\n",
    "s_hex = ''\n",
    "for byte in s_bytes:\n",
    "    s_hex += str(hex(byte)[2:]) + ' '\n",
    "print(s_hex)\n",
    "\n",
    "# Or just do this\n",
    "print(s_bytes.hex())\n",
    "\n",
    "# Inverse operation is fromhex\n",
    "print(bytes.fromhex(s_bytes.hex()))\n",
    "print(bytes.fromhex(s_hex))"
   ]
